{
  "builds": [
    {
      "src": "api.js",
      "use": "@vercel/node@2"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "api.js"
    },
    {
      "src": "/",
      "dest": "index.html"
    }
  ],
  "functions": {
    "api.js": {
      "runtime": "nodejs22.x"
    }
  }
}
